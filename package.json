{
  "name": "rock-native",
  "version": "0.0.1",
  "license": "SEE LICENSE IN LICENSE",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "coverage": "npm run test-unit -- --coverage && codecov",
    "test": "jest \"^((?!integration).)*$\"",
    "test-unit": "jest \"^((?!integration).)*$\"",
    "test-int": "jest \"*.integration.js\"",
    "test-all": "jest",
    "lint": "eslint ./src index.*",
    "lint-staged": "lint-staged",
    "flow": "flow",
    "flow-stop": "flow stop",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "windows": "react-native run-windows",
    "web": "cd ./web && cross-env ASPNETCORE_ENVIRONMENT=development dotnet watch run --project ./RockNative.csproj --server.urls \"http://*:3000\"",
    "web-build": "cd ./web && cross-env ASPNETCORE_ENVIRONMENT=Production dotnet publish --configuration Release",
    "now": "npm run web-build && now ./web --docker -e ASPNETCORE_ENVIRONMENT=Production -e NODE_ENV=production",
    "danger": "danger run --verbose",
    "restore": "cd web && dotnet restore",
    "stat-vendor": "webpack-bundle-analyzer ./web/wwwroot/app/dist/vendor-stats.json",
    "stat-app": "webpack-bundle-analyzer ./web/wwwroot/dist/stats.json"
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "jest --bail --findRelatedTests"
    ]
  },
  "pre-commit": "lint-staged",
  "dependencies": {
    "@jongold/further": "^1.0.3",
    "aspnet-prerendering": "^2.0.3",
    "domain-task": "^3.0.0",
    "event-source-polyfill": "0.0.9",
    "preact": "^8.1.0",
    "preact-compat": "^3.15.0",
    "ramda": "^0.23.0",
    "react": "^15.4.2",
    "react-helmet": "^5.0.3",
    "react-native": "0.42.3",
    "react-native-windows": "^0.42.0",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "recompose": "^0.22.0"
  },
  "devDependencies": {
    "aspnet-webpack": "^1.0.28",
    "aspnet-webpack-react": "^1.0.5",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.1",
    "babel-jest": "19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-root-import": "^5.1.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-react-require": "^1.0.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-native": "1.9.1",
    "codecov": "^2.1.0",
    "cross-env": "^4.0.0",
    "danger": "https://github.com/delianides/danger-js/releases/download/0.14.2/danger-0.14.2.tgz",
    "enzyme": "^2.8.0",
    "enzyme-to-json": "^1.5.0",
    "eslint": "^3.18.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-prettier": "^2.0.1",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-react-native": "^2.3.1",
    "flow-bin": "^0.38.0",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "lint-staged": "^3.4.0",
    "now": "^4.10.2",
    "offline-plugin": "^4.7.0",
    "pre-commit": "^1.2.2",
    "prettier": "^0.22.0",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "~15.4.1",
    "rnpm-plugin-windows": "^0.2.4",
    "selenium-webdriver": "^3.3.0",
    "webpack": "^2.3.0",
    "webpack-bundle-analyzer": "^2.4.0",
    "webpack-dev-server": "^2.4.2",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^4.1.0"
  },
  "jest": {
    "preset": "react-native",
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!**/node_modules/**",
      "!src/test-utils/**"
    ]
  }
}
